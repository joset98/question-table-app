{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
		"jest"
    ],
    "rules": {
        "react/react-in-jsx-scope": 0,
        "react/jsx-uses-react": 0
    }
}
